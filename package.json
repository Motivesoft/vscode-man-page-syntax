{
  "name": "vscode-man-page-syntax",
  "displayName": "Man Page Syntax Highlighting",
  "description": "Language support for man pages",
  "version": "1.1.1",
  "keywords": [
    "man",
    "mdoc",
    "man page"
  ],
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "publisher": "Motivesoft",
  "author": {
    "name": "ianabrown",
    "url": "https://github.com/ianabrown"
  },
  "homepage": "https://github.com/Motivesoft/vscode-man-page-syntax/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/Motivesoft/vscode-man-page-syntax.git"
  },
  "bugs": {
    "url": "https://github.com/Motivesoft/vscode-man-page-syntax/issues"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "man",
        "aliases": [
          "Man Page",
          "man",
          "manpage"
        ],
        "extensions": [
          ".man",
          ".mdoc",
          ".1",
          ".2",
          ".3",
          ".4",
          ".5",
          ".6",
          ".7",
          ".8",
          ".9"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "man",
        "scopeName": "text.manpage",
        "path": "./syntaxes/man.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "man",
        "path": "./snippets/snippets.code-snippets"
      }
    ],
    "configuration": {
      "files.associations": {
        "*.man": "man",
        "*.mdoc": "man",
        "*.1": "man",
        "*.2": "man",
        "*.3": "man",
        "*.4": "man",
        "*.5": "man",
        "*.6": "man",
        "*.7": "man",
        "*.8": "man",
        "*.9": "man"
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.101.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3"
  },
  "dependencies": {
  }
}
