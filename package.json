{
  "name": "vscode-man-page-syntax",
  "displayName": "Man Page Syntax Highlighting",
  "description": "Language support for man pages",
  "version": "1.0.4",
  "keywords": [
    "man",
    "mdoc",
    "man page"
  ],
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "publisher": "Motivesoft",
  "author": {
    "name": "ianabrown",
    "url": "https://github.com/ianabrown"
  },
  "homepage": "https://github.com/Motivesoft/vscode-man-page-syntax/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/Motivesoft/vscode-man-page-syntax.git"
  },
  "bugs": {
    "url": "https://github.com/Motivesoft/vscode-man-page-syntax/issues"
  },
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "man",
        "aliases": [
          "Man Page",
          "man",
          "manpage"
        ],
        "extensions": [
          ".man",
          ".mdoc",
          ".1",
          ".2",
          ".3",
          ".4",
          ".5",
          ".6",
          ".7",
          ".8",
          ".9"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "man",
        "scopeName": "text.manpage",
        "path": "./syntaxes/man.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "man",
        "path": "./snippets/snippets.code-snippets"
      }
    ],
    "configuration": {
      "files.associations": {
        "*.man": "man",
        "*.mdoc": "man",
        "*.1": "man",
        "*.2": "man",
        "*.3": "man",
        "*.4": "man",
        "*.5": "man",
        "*.6": "man",
        "*.7": "man",
        "*.8": "man",
        "*.9": "man"
      }
    }
  },
  "scripts": {
    "compile": "tsc -p .",
    "watch": "tsc -watch -p .",
    "vscode:prepublish": "npm run compile"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "esbuild": "^0.25.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3"
  }
}
